{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": ["error", { "code": 100 }],
        "require-jsdoc": "off",
        "no-multi-str": "off"
    }
}
